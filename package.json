{
  "name": "backend-app-template",
  "version": "1.0.0",
  "description": "Production-grade Node.js/Express backend template with TypeScript",
  "main": "dist/server.js",
  "_moduleAliases": {
    "@controllers": "dist/controllers",
    "@services": "dist/services",
    "@models": "dist/models",
    "@routes": "dist/routes",
    "@middlewares": "dist/middlewares",
    "@configs": "dist/configs",
    "@shared": "dist/shared",
    "@utils": "dist/utils",
    "@requests": "dist/requests"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js"
  },
  "keywords": [
    "express",
    "typescript",
    "nodejs",
    "backend",
    "template"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongoose": "^8.0.0",
    "multer": "^2.0.2",
    "node-cache": "^5.1.2",
    "openai": "^6.15.0",
    "pdf-parse": "^2.4.5",
    "pdfdataextract": "^4.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.5",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
